nar\",\"Similar\":{\"SimilarCount\":\"1\",\"Game\":{\"id\":\"45064\",\"PlatformId\":\"23\"}},\"Images\":{\"boxart\":{\"_\":\"boxart/original/front/40587-1.jpg\",\"$\":{\"side\":\"front\",\"width\":\"416\",\"height\":\"595\",\"thumb\":\"boxart/thumb/original/front/40587-1.jpg\"}}}}","Major League Baseball":"{\"id\":\"1897\",\"GameTitle\":\"Major League Baseball\",\"PlatformId\":\"7\",\"Platform\":\"Nintendo Entertainment System (NES)\",\"ReleaseDate\":\"04/01/1988\",\"Overview\":\"Sure you've always wanted to be a professional ball player but have you ever thought about managing? Well, here's your chance to prove yourself both on and off the field.\",\"Genres\":{\"genre\":\"Sports\"},\"Players\":\"1\",\"Co-op\":\"No\",\"Publisher\":\"LJN\",\"Developer\":\"Atlus\",\"Rating\":\"7\",\"Similar\":{\"SimilarCount\":\"1\",\"Game\":{\"id\":\"10498\",\"PlatformId\":\"32\"}},\"Images\":{\"boxart\":{\"_\":\"boxart/original/front/1897-1.jpg\",\"$\":{\"side\":\"front\",\"width\":\"350\",\"height\":\"495\",\"thumb\":\"boxart/thumb/original/front/1897-1.jpg\"}},\"clearlogo\":{\"_\":\"clearlogo/1897.png\",\"$\":{\"width\":\"400\",\"height\":\"150\"}}}}","Majou Densetsu II - Daimashikyou Galious":"{\"id\":\"41297\",\"GameTitle\":\"Majou Densetsu II: Daimashikyou Galious\",\"PlatformId\":\"7\",\"Platform\":\"Nintendo Entertainment System (NES)\",\"Overview\":\"Majou Densetsu II: Daimashikyou Galious is a Role-Playing game, developed and published by Konami, which was released in Japan in 1987.\",\"Players\":\"1\",\"Co-op\":\"No\",\"Publisher\":\"Konami\",\"Developer\":\"Konami\",\"Images\":{\"boxart\":{\"_\":\"boxart/original/front/41297-1.jpg\",\"$\":{\"side\":\"front\",\"width\":\"640\",\"height\":\"448\",\"thumb\":\"boxart/thumb/original/front/41297-1.jpg\"}}}}","Make NES Music Demo (PD)":{},"Make NES Music Demo (PD) ":{},"Mandelbrot (PD)":{},"Manhole by KZ-S (PD)":{},"Manhole by KZ-S (PD) ":{},"Maniac Mansion":"{\"id\":\"1432\",\"GameTitle\":\"Maniac Mansion\",\"PlatformId\":\"7\"